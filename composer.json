{
  "name": "codexten/yii-module-kpi",
  "type": "library",
  "description": "kpi module for yii 2.0 or higher",
  "require": {
    "codexten/yii-web": "~2.0.0",
    "codexten/yii-dataview": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\kpi\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "core": "config/core.php",
      "modulekpi": "config/modulekpi.php"
    }
  }
}
